      attribute 10861324 "mUpdateChannelListRunnable"
	const_attribute private explicit_type "Runnable"
	init_value " new Runnable() {
        @Override
        public void run() {
            // update GTV channel list database
            try {
                TvFramework.getInstance().getGtvSpecificInstance()
                        .updateQSB(mContext, PairingDevice.TUNER_DEVICE_ID);

                if (mContext != null) {
                    // dismiss progress dialog
                    Intent intent = new Intent(TvIntents.ACTION_SHOW_PROGRESS);
                    intent.putExtra(TvIntents.EXTRA_PROGRESS_STATUS, false);
                    mContext.startService(intent);
                }
            } catch (Exception e) {
                Log.e(TAG, \"Error in Channel List update\", e);
            }
        }
    }"
	cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
