      classrelation 5586188 // mHandler (<unidirectional association>)
	relation 5586060 --->
	  a role_name "mHandler" init_value " new Handler(Looper.getMainLooper()) {

        @Override
        public void handleMessage(Message msg) {
            if (msg != null && msg.what == CHANNEL_SURFING_TIMEOUT_MESSAGEID) {
                Log.d(TAG, \"timeout, exit surfing\");
                exitSurfingMode(true);
            }
        }
    }" const_relation private
