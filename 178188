	  param inout name "intent" type class_ref 678156 // Intent
	  param inout name "onFinished" type class_ref 791436 // OnFinished
	  param inout name "handler" type class_ref 687500 // Handler
	  param in name "requiredPermission" explicit_type "String"
	nexceptions 1
	  exception class_ref 687244 // SendIntentException
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
	cpp_def "${comment}${inline}${type} ${class}::${name}${(}${)}${const}${volatile}${throw}${staticnl}{
  ${body}}
