      operation 2119436 "createConfigurationContext"
	abstract public return_type class_ref 684812 // Context
	nparams 1
	  param inout name "overrideConfiguration" type class_ref 688012 // Configuration
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
	java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${t0} ${p0}${)}${throws}${staticnl}{
  ${body}}
