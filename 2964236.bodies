class StandbyTestActivity
!!!12010764.java!!!	onKeyDown(in keyCode : int, inout event : KeyEvent) : boolean
        if (keyCode == KeyEvent.KEYCODE_0) {
             TvSettings.System.setWakeUpTimer(6000);
            Log.d(TAG, "setWakeUpTimer");
        } else if (keyCode == KeyEvent.KEYCODE_1) {
            TvSettings.System.setStandbyTimer(6000);
            Log.d(TAG, "setStandbyTimer");
        }
        return super.onKeyDown(keyCode, event);
