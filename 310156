      operation 11258380 "mouseMoved"
	public explicit_return_type "void"
	nparams 5
	  param in name "windowWidth" explicit_type "uint"
	  param in name "windowHeight" explicit_type "uint"
	  param in name "buttonStateMask" explicit_type "int"
	  param in name "x" explicit_type "int"
	  param in name "y" explicit_type "int"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}, ${t2} ${p2}${v2}, ${t3} ${p3}${v3}, ${t4} ${p4}${v4}${)}${const}${volatile}${throw}${abstract};
"
	cpp_def "${comment}${inline}${type} ${class}::${name}${(}${t0} ${p0}, ${t1} ${p1}, ${t2} ${p2}, ${t3} ${p3}, ${t4} ${p4}${)}${const}${throw}${staticnl}{
  ${body}}
"
	java_def "  ${comment}${@}${visibility}${final}${static}${abstract}${synchronized}${type} ${name}${(}${)}${throws}${staticnl}{
  ${body}}
"
	
	
	
	comment "*
	 * Callback when mouse is moved in video area. Determines if mouse movement should be sent, in
	 * what type (generic or HIDC) and sends it to the source.
	 * \\param windowWidth Width of the window containing video (used to calculate padding, determine
	 * if it is necessary to send UIBC event at all and rescale coordinates properly)
	 * \\param windowHeight Height of the window containing video (used to calculate padding, determine
	 * if it is necessary to send UIBC event at all and rescale coordinates properly)
	 * \\param buttonStateMask Bitmask for clicked mouse buttons (of MouseWindowButtons),
	 * used only for HIDC event
	 * \\param x Current x position of mouse, top-left corner is origin
	 * \\param y Current y position of mouse, top-left corner is origin"
      end

