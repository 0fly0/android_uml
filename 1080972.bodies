                    while ((!receiver.isDone()) && (waitTime < MAX_WAIT_TIME)) {
                        receiver.wait(WAIT_TIME_INCR);
                        waitTime += WAIT_TIME_INCR;
                    }
                    if (!receiver.isDone()) {
