      attribute 10811020 "mChannelChangeRunnable"
	const_attribute private explicit_type "Runnable"
	init_value " new Runnable() {

        @Override
        public void run() {
            duringChannelNavigation = true;

            /**
             * do not close the activity, just make it invisible, scan progress
             * will be displayed in the activity context
             */
            LinearLayout layout = (LinearLayout) findViewById(R.id.layout_panel);
            layout.setVisibility(View.INVISIBLE);

            try {
                mMajorNumber = mTextViewMajor.getText().toString();
                mMinorNumber = mTextViewMinor.getText().toString();

                // Ignore if major number is 0 and minor number is 0 or not
                // entered .
                if ((Integer.parseInt(mMajorNumber) == 0)
                        && (mMinorNumber == null || mMinorNumber.length() == 0 || Integer
                                .parseInt(mMinorNumber) == 0)) {
                    clearAll();

                } else {
                    if (mMinorNumber == null || mMinorNumber.length() == 0) {
                        // mMinorNumber = mMajorNumber;
                        // mMajorNumber =
                        // Integer.toString(mRes.getInteger(R.integer.invalid_major_no));

                        // fix Trestles Bug 1024 - ChannelChange API requires
                        // channel number as
                        // major=x minor=65535 for analog channel or
                        // one-part-number channel
                        mMinorNumber = Integer
                                .toString(mRes.getInteger(R.integer.invalid_major_no));
                        Log.d(TAG, \"Only major number \" + mMajorNumber);
                    }

                    ChannelDetails currChDetail = mLive.getCurrentChannelDetail();
                    final USDTVMessageQueue mUSDTVMessageQueue = USDTVMessageQueue
                            .getMsgQueueInstance();
                    final Events signalEvent = mUSDTVMessageQueue
                            .getEventByGroup(EventGroup.EVENT_GROUP_SIGNAL);
                    final Events blockEvent = mUSDTVMessageQueue
                            .getEventByGroup(EventGroup.EVENT_GROUP_BLOCK);

                    Wrapper.getWrapperInstance().pauseCC();
                    int returnValue = mLive.channelChange(false, Integer.parseInt(mMajorNumber),
                            Integer.parseInt(mMinorNumber), null);
                    if (returnValue == ErrorCodes.FAILURE
                            && Integer.parseInt(mMinorNumber) == mRes
                                    .getInteger(R.integer.invalid_major_no)) {
                        returnValue = startManualScan(mMajorNumber, mMinorNumber,
                                ChannelSearchType.SEARCH_DIGITAL);
                    } else {
                        clearAll();

                        if (returnValue == ErrorCodes.SUCCESS) {
                            // Even if channelChange returns SUCCESS, channel
                            // may be not really changed.
                            // But CC is paused and CURRENT_CHANNEL_PLAY_SUCCESS
                            // event will be sent to UI,
                            // so we need to do some recovery here
                            ChannelDetails newChDetail = mLive.getCurrentChannelDetail();
                            if (Wrapper.getWrapperInstance().isSameChannel(currChDetail,
                                    newChDetail)) {
                                // resume CC
                                returnValue = ErrorCodes.FAILURE;

                                // resume signal events if current channel has
                                // signal and not blocked
                                mHandler.postDelayed(new Runnable() {
                                    @Override
                                    public void run() {
                                        if (!EventManagerService.isNoSignal(signalEvent)
                                                && !EventManagerService.isBlocked(blockEvent)) {
                                            mUSDTVMessageQueue.addEventToGroup(signalEvent,
                                                    EventGroup.EVENT_GROUP_SIGNAL);
                                            Intent intent = new Intent(
                                                    EventManagerService.ACTION_CHECK_EVENTS);
                                            intent.putExtra(EventManagerService.EXTRA_EVENT_GROUP,
                                                    EventGroup.EVENT_GROUP_SIGNAL);
                                            sendBroadcast(intent);
                                        }
                                    }
                                }, 500);
                            }
                        } else if (returnValue == ErrorCodes.USDTV_CHANNEL_LOCKED) {
                            final String text = mRes.getString(R.string.scan_fail_locked_channel);
                            new ToastCreator(mContext, text, R.drawable.info).showToast();
                        }
                    }

                    // fix Trestles bug31683 resume CC if channel is not changed
                    // or scaned
                    if (returnValue == ErrorCodes.FAILURE) {
                        Wrapper.getWrapperInstance().resumeCC();
                    }
                }
            }
            catch (NumberFormatException e) {
                ProgramaticException.displayException(mContext, TAG, e);
            }
        }
    }"
	cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
"
	java_decl "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	php_decl ""
	python_decl ""
	idl_decl ""
      end

