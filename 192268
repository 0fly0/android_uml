      classrelation 1766540 // mListOnKeyListener (<unidirectional association>)
	relation 1766540 --->
	  a role_name "mListOnKeyListener" init_value " new OnKeyListener() {

        @Override
        public boolean onKey(View v, int keyCode, KeyEvent event) {
            Object selectedItem = mList.getSelectedItem();
            if (selectedItem instanceof Preference) {
                View selectedView = mList.getSelectedView();
                return ((Preference)selectedItem).onKey(
                        selectedView, keyCode, event);
            }
            return false;
        }

    }" private
	    cpp default "    ${comment}${static}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 1766540 // mListOnKeyListener (<unidirectional association>)
	  b parent class_ref 699532 // OnKeyListener
      end
    end

