      classrelation 5516172 // mTrackSelectionListener (<unidirectional association>)
	relation 5516044 --->
	  a role_name "mTrackSelectionListener" init_value " new View.OnClickListener() {
        int[] mIndex;
        String[] mTrackNames;
        public void onClick(View v) {
            new AlertDialog.Builder(mContext)
            .setTitle(mTrackSelectionString)
            .setItems(mTrackMenu, new DialogInterface.OnClickListener() {
                public void onClick(DialogInterface dialog, int which) {
                    if (mTrackMenu[which].compareTo(mVideoTrackString) == 0) {
                        mIndex = mVideoTrackIndex;
                        mTrackNames = mVideoTrackNames;
                    } else if (mTrackMenu[which].compareTo(mAudioTrackString) == 0) {
                        mIndex = mAudioTrackIndex;
                        mTrackNames = mAudioTrackNames;
                    } else if (mTrackMenu[which].compareTo(mSubtitleTrackString) == 0) {
                        mIndex = mTimedTextTrackIndex;
                        mTrackNames = mTimedTextTrackNames;
                    } else {
                        Log.e(TAG, \"No avaliable track type in the menu.\");
                        return;
                    }

                    // Choose track dialog
                    new AlertDialog.Builder(mContext)
                    .setTitle(mChooseTrackString)
                    .setItems(mTrackNames, new DialogInterface.OnClickListener() {
                        public void onClick(DialogInterface dialog, int which) {
                            mPlayer.selectTrack(mIndex[which]);
                        }
                    })
                    .show();

                }
            })
            .show();
        }
    }" private
	    cpp default "    ${comment}${static}${mutable}${volatile}${const}${type} * ${name}${value};
"
