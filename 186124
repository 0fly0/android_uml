      operation 2984716 "updateAppWidgetSize"
	public explicit_return_type "void"
	nparams 5
	  param inout name "newOptions" type class_ref 678924 // Bundle
	  param in name "minWidth" explicit_type "int"
	  param in name "minHeight" explicit_type "int"
	  param in name "maxWidth" explicit_type "int"
	  param in name "maxHeight" explicit_type "int"
