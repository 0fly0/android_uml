	cpp_decl "    ${comment}${inline}${virtual}${name}${(}${)}${volatile}${throw} {
        DrmMetadata::KeyIterator keyIt = this->keyIterator();

        while (keyIt.hasNext()) {
            String8 key = keyIt.next();
            const char* value = this->getAsByteArray(&key);
            if (NULL != value) {
                delete[] value;
                value = NULL;
