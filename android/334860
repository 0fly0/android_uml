      classrelation 5480716 // mHandler (<unidirectional association>)
	relation 5480588 --->
	  a role_name "mHandler" init_value " new Handler() {
        @Override
        public void handleMessage(Message msg) {
            switch (msg.what) {
                case REQ_UPDATE_CEC_DEV_LIST:
                    if (msg.obj instanceof List<?>) {
                        if (DEBUG) {
                            Log.d(TAG, \"Handling the REQ_UPDATE_CEC_DEV_LIST devList:\"
                            + msg.obj + \" activeAddr:\" + msg.arg1);
                        }
                        HdmiCecSettings.this.updateCecDeviceList(
                        (List<CECDevInfo>)msg.obj, msg.arg1);
                    }
                    break;
                case REQ_UPDATE_CEC_STATUS:
                    break;
                default:
                    Log.e(TAG, \"unknown req:\" + msg.what);
                    break;
            }
        }
    }" private
	    cpp default "    ${comment}${static}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 5480716 // mHandler (<unidirectional association>)
	  b parent class_ref 687500 // Handler
      end

