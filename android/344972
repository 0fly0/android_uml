      classrelation 5645196 // mHandler (<unidirectional association>)
	relation 5645068 --->
	  a role_name "mHandler" init_value " new Handler() {
        public void handleMessage(Message msg) {
            Bundle b = msg.getData();
            String str;
            if (null != (str = b.getString(\"seek\"))) {
