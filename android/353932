      classrelation 5773708 // mConnection (<unidirectional association>)
	relation 5773580 --->
	  a role_name "mConnection" init_value " new ServiceConnection() {
        @Override
        public void onServiceConnected(ComponentName className,
                IBinder service) {
            Log.v(TAG, \"onServiceConnected: \" + className + \" IBinder \" +
                service);

            mService = IDiscoveryService.Stub.asInterface(service);

            /*
             * bindService() is asynchronous call. Client may have already registered advertiser
             * or listener before connection to remote service is established, e.g.
             * ServiceConnection() is called.
             *
             * To handle such case, use mQueuedAdvertisers and mQueuedListeners to store the
             * service request from client. Once connection is established, those 2 queues will be
             * dequeued and service request will be sent to remote service.
             */
            synchronized (mQueuedAdvertisers) {
                for (Iterator iter = mQueuedAdvertisers.iterator(); iter.hasNext();) {
                    ServiceRecord record = (ServiceRecord) iter.next();
                    registerAdvertiserRemote(record);
                }
                mQueuedAdvertisers.clear();
            }
            synchronized (mQueuedListeners) {
                for (RecordType key: mQueuedListeners.keySet()) {
                    registerListenerRemote(key, mQueuedListeners.get(key));
                }
                mQueuedListeners.clear();
            }
        }

        @Override
        public void onServiceDisconnected(ComponentName className) {
            Log.v(TAG, \"onServiceDisconnected\");
