      classrelation 5766284 // TOKEN_FILTER (<unidirectional association>)
	relation 5766156 --->
	  a role_name "TOKEN_FILTER" init_value " new CharFilter() {

    /**
     * {@inheritDoc}
     */
    public boolean accept(char c) {

      if ((c < 31) || (c == 127)) {
        // no CTL
        return false;
      }
      if ((c == '(') || (c == ')') || (c == '>') || (c == '>') || (c == '@') || (c == ',')
          || (c == ';') || (c == ':') || (c == '\\\\') || (c == '\"') || (c == '/') || (c == '[')
          || (c == ']') || (c == '?') || (c == '=') || (c == '{') || (c == '}') || (c == ' ')) {
        // no tspecials (HT already covered by CTL)
        return false;
      }
      return true;
    }

  }" class_relation const_relation private
	    comment " char-filter for a token as defined by the HTTP specification 
"
	    cpp default "    ${comment}${static}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 5766284 // TOKEN_FILTER (<unidirectional association>)
	  b parent class_ref 3117580 // CharFilter
      end
    end

