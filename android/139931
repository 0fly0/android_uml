      operation 2185371 "getHash"
	public explicit_return_type "long"
	nparams 1
	  param inout name "arr" explicit_type "byte"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
