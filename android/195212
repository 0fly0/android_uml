      operation 6670092 "startActivity"
	protected explicit_return_type "T"
	nparams 3
	  param inout name "intent" type class_ref 678156 // Intent
	  param inout name "savedInstanceState" type class_ref 678924 // Bundle
	  param inout name "lastNonConfigurationInstance" explicit_type "Object"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
