      operation 681371 "asReadOnlyBuffer"
	abstract public return_type class_ref 180635 // IntBuffer
	nparams 0
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
