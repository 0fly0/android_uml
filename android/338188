      classrelation 5532172 // mToastHandler (<unidirectional association>)
	relation 5532044 --->
	  a role_name "mToastHandler" init_value " new Handler() {
            public void handleMessage(Message msg) {
                switch (msg.arg1) {
                case KeyEvent.KEYCODE_VOLUME_UP:
                    showToast(R.string.avr_volume_up);
                    break;
                case KeyEvent.KEYCODE_VOLUME_DOWN:
                    showToast(R.string.avr_volume_down);
                    break;
                case KeyEvent.KEYCODE_VOLUME_MUTE:
                    showToast(R.string.avr_volume_mute);
                    break;
                case MSG_POWER:
                    showToast(R.string.fail_to_control_power);
                    break;
                default:
                    break;
                }
            }
        }" const_relation private
