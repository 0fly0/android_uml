	operation 2844684 "getPresentationDisplay"
	  public return_type class_ref 688140 // Display
	  nparams 0
	  cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
