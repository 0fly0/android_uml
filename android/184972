      operation 6858380 "createImageThumbnail"
	class_operation public return_type class_ref 686860 // Bitmap
	nparams 2
	  param in name "filePath" explicit_type "String"
	  param in name "kind" explicit_type "int"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
