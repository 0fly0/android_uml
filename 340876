      attribute 10833292 "mTimerExpiredRunnable"
	const_attribute private explicit_type "Runnable"
	init_value " new Runnable() {
        @Override
        public void run() {
            isTimerActive = false;

            if (!WidgetManager.isTopActivity(mContext, NotifyShutdownActivity.class.getName())) {
                WidgetManager.exitFromApp(mContext);

                Intent intent = new Intent(TvIntents.ACTION_SHOW_NOTIFY_SHUTDOWN_ACTIVITY);
                intent.addFlags(Intent.FLAG_ACTIVITY_NEW_TASK);
                intent.putExtra(NotifyShutdownActivity.EXTRA_ACTION_CANCEL, getCancelAction());
                startActivity(intent);

                mHandler.postDelayed(mShutdownRunnable, Utils.ONE_MINUTE);
            }
        }
    }"
