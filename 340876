      classrelation 5576460 // mHandler (<unidirectional association>)
	relation 5576332 --->
	  a role_name "mHandler" init_value " new Handler() {
        @Override
        public void handleMessage(Message msg) {
            super.handleMessage(msg);
            Log.d(TAG, \"handleMessage=\" + msg.what);
            switch (msg.what) {
                case SHOW_NO_SIGNAL: {
                    createAlertDialog(SHOW_NO_SIGNAL);
                    break;
                }
                case SHOW_INPUT_STABLE: {
                    destroyAlertDialog(ALERT_TYPE_FORCE);
                    break;
                }
                case SHOW_CHANNEL_BANNER: {
                    if (canShowOSD(SHOW_CHANNEL_BANNER)) {
                        WidgetManager.showChannelBanner(mContext);
                    }
                    break;
                }
                case SHOW_INPUT_BANNER: {
                    if (canShowOSD(SHOW_INPUT_BANNER)) {
                        tryTimes = 0;
                        WidgetManager.showInputBanner(mContext, true, false);
                    } else {
                        // bug27869: wait a while to check again
                        // if info banner can't show right now
                        if (tryTimes++ > MAXIMUM_TRY_TIMES) {
                            tryTimes = 0;
                            break;
                        }

                        mHandler.sendEmptyMessageDelayed(SHOW_INPUT_BANNER, 1000);
                    }
                    break;
                }
                case SHOW_BLOCK: {
                    if (canShowOSD(SHOW_BLOCK)) {
                        if (Wrapper.getWrapperInstance().isTunerInput()) {
                            if (!WidgetManager.checkActivityRunning(mContext,
                                    ChannelBanner.class.getName())) {
                                WidgetManager.showChannelBanner(mContext);
                            }
                        } else {
                            if (!WidgetManager.checkActivityRunning(mContext,
                                    PinActivity.class.getName())) {
                                PCUtility.playCurrentSource(mContext, true, true);
                            }
                        }
                    }
                    break;
                }
                case SHOW_UNBLOCK: {
                    PCUtility.removeChannelLockScreen(mContext);

                    if (canShowOSD(SHOW_UNBLOCK)) {
                        if (Wrapper.getWrapperInstance().isTunerInput()) {
                            if (!WidgetManager.checkActivityRunning(mContext,
                                    ChannelBanner.class.getName())) {
                                WidgetManager.showChannelBanner(mContext);
                            }
                        }
                    }
                    break;
                }
                case SHOW_SERVICE_DELETED: {
                    createAlertDialog(SHOW_SERVICE_DELETED);
                    break;
                }
                case SHOW_AV_INSTABLE: {
                    createAlertDialog(SHOW_AV_INSTABLE);
                    break;
                }
            }
        }
    }" const_relation private
	    comment " General Handler for all messages ************************
"
	    cpp default "    ${comment}${static}${mutable}${volatile}${const}${type} * ${name}${value};
"
	    java "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	    classrelation_ref 5576460 // mHandler (<unidirectional association>)
	  b parent class_ref 687500 // Handler
      end

