      attribute 10836620 "checkSignalEventRunnable"
	const_attribute private explicit_type "Runnable"
	init_value " new Runnable() {
        @Override
        public void run() {
            if (mNoSignalTimerTask != null) {
                mNoSignalTimerTask.cancel();
                mNoSignalTimerTask = null;
            }
            if (mAVInstableTimerTask != null) {
                mAVInstableTimerTask.cancel();
                mAVInstableTimerTask = null;
            }

            Events signalEvent = mUSDTVMessageQueue
                    .getEventByGroup(EventGroup.EVENT_GROUP_SIGNAL);
            if (signalEvent == null)
                return;

            boolean isTuner = Wrapper.getWrapperInstance().isTunerInput();

            // Show/Dismiss No signal warning according to signal events
            if (isNoSignal(signalEvent)) {
                removeBlockEvent(); // no signal event can override block event

                mNoSignalTimerTask = new TimerTask() {
                    @Override
                    public void run() {
                        mHandler.sendEmptyMessage(SHOW_NO_SIGNAL);
                    }
                };

                int firstShow = 15 * SEC;
                int repeatShow = 5 * SEC;
                if (isTuner) {
                    firstShow = 3 * SEC;
                    repeatShow = 3 * SEC;
                }
                if (mTimer != null)
                    mTimer.schedule(mNoSignalTimerTask, firstShow, repeatShow);

            } else if (!isAvStable(signalEvent)) {
                if (signalEvent == Events.NO_AUDIO_COMPONENT) {
                    // one time
                    // mHandler.sendEmptyMessage(SHOW_AV_INSTABLE);
                    new ToastCreator(mContext, mContext.getResources().getString(
                            R.string.no_audio_signal),
                            R.drawable.info).showToast();

                } else {
                    // always show
                    mAVInstableTimerTask = new TimerTask() {
                        @Override
                        public void run() {
                            mHandler.sendEmptyMessage(SHOW_AV_INSTABLE);
                        }
                    };

                    if (mTimer != null)
                        mTimer.schedule(mAVInstableTimerTask, 0, 3 * SEC);
                }
            } else {
                mHandler.sendEmptyMessage(SHOW_INPUT_STABLE);
            }

            if (isTuner) {
                // Show Channel Banner when changing channel or channel video
                // signal is acquired
                if (signalEvent == Events.CURRENT_CHANNEL_PLAY_SUCCESS
                        || signalEvent == Events.RF_RESOLUTION_CHANGE) {
                    mHandler.sendEmptyMessage(SHOW_CHANNEL_BANNER);

                } else if (signalEvent == Events.TS_DELETED_SERVICE_EVENT) {
                    mHandler.sendEmptyMessage(SHOW_SERVICE_DELETED);
                    mHandler.sendEmptyMessage(SHOW_CHANNEL_BANNER);

                }
            } else {
                // Show Input Banner when changing input or input video signal
                // is acquired
                if (signalEvent == Events.STOP_SOURCE_DONE
                        || signalEvent == Events.SCM_VIDEO_RESOLUTION_CHANGE) {
                    tryTimes = 0;
                    mHandler.sendEmptyMessage(SHOW_INPUT_BANNER);

                }
            }
        }
    }"
	cpp_decl "    ${comment}${static}${mutable}${volatile}${const}${type} ${name}${value};
"
	java_decl "  ${comment}${@}${visibility}${static}${final}${transient}${volatile}${type} ${name}${value};
"
	php_decl ""
	python_decl ""
	idl_decl ""
	comment " Signal Functions ***************************************"
      end

